{
  "displayName": "Homebridge Arlo",
  "name": "homebridge-arlo",
  "version": "2.1.0",
  "description": "Arlo plugin for homebridge",
  "author": "homebridge-plugins",
  "main": "dist/index.js",
  "scripts": {
    "developer": "DEBUG=arlo homebridge -D -U `pwd` -P `pwd`",
    "lint": "eslint src/**/*.{js,ts,json}",
    "watch": "npm run build && npm link && nodemon",
    "build": "npm run clean && tsc",
    "test": "jest",
    "clean": "rimraf ./dist",
    "prepublishOnly": "npm run lint && npm run build",
    "postpublish": "npm run clean"
  },
  "license": "ISC",
  "keywords": [
    "homebridge-plugin",
    "arlo",
    "homebridge"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/homebridge-plugins/homebridge-arlo.git"
  },
  "bugs": {
    "url": "http://github.com/homebridge-plugins/homebridge-arlo/issues"
  },
  "engines": {
    "node": ">=8.15.1",
    "homebridge": ">=1.0.0"
  },
  "devDependencies": {
    "@types/ip": ">=1.1.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "homebridge": "^1.1.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "ffmpeg-for-homebridge": ">=0.0.7",
    "node-arlo": ">=1.3.1.beta0",
    "ip": ">=1.1.5"
  }
}
